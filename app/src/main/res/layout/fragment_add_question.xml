<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/material_blue_400"
    tools:context="com.example.nefrin.newprojectstartup.MainActivity">

    <EditText
        android:id="@+id/first__part"
        android:layout_width="318dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|top"
        android:hint="first part" />

    <EditText
        android:id="@+id/second__part"
        android:layout_width="299dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:hint="but part" />

    <Button
        android:id="@+id/send_q"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|bottom"
        android:text="send_q" />
</FrameLayout>